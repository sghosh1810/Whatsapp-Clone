<% if(typeof dashboard_toast != 'undefined'){ %>
    <% if(dashboard_toast.type == 'warning'){ %>
        <div id="toast-profile-update" class="toast ml-auto" role="alert" style="z-index: 999; top: 10%;">
            <div class="bg-warning toast-header">
                <strong class="mr-auto text-white"><%= dashboard_toast.header %></strong>
                <small class="text-white"><%= dashboard_toast.time %></small>
                <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="bg-warning toast-body text-white"> <%= dashboard_toast.message %> </div>
        <div>
    <% } %>
    <% if(dashboard_toast.type == 'danger'){ %>
        <div id="toast-profile-update" class="toast ml-auto" role="alert" style="z-index: 999; top: 10%;">
            <div class="bg-danger toast-header">
                <strong class="mr-auto text-white"><%= dashboard_toast.header %></strong>
                <small class="text-white"><%= dashboard_toast.time %></small>
                <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="bg-danger toast-body text-white"> <%= dashboard_toast.message %> </div>
        <div>
    <% } %>
    <% if(dashboard_toast.type == 'success'){ %>
        <div id="toast-profile-update" class="toast ml-auto" role="alert" style="z-index: 999; top: 10%;">
            <div class="bg-success toast-header">
                <strong class="mr-auto text-white"><%= dashboard_toast.header %></strong>
                <small class="text-white"><%= dashboard_toast.time %></small>
                <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="bg-success toast-body text-white"> <%= dashboard_toast.message %> </div>
        <div>
    <% } %>
<% } %>